  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "dependencies": {
    "@nestia/fetcher": "^1.5.3",
    "@nestjs/common": ">= 7.0.1",
    "@nestjs/core": ">= 7.0.1",
    "@nestjs/platform-express": ">= 7.0.1",
    "@nestjs/platform-fastify": ">= 7.0.1",
    "detect-ts-node": "^1.0.5",
    "glob": "^7.2.0",
    "raw-body": ">= 2.0.0",
    "reflect-metadata": ">= 0.1.12",
    "rxjs": ">= 6.0.0",
    "typia": "^4.1.15"
  },
  "peerDependencies": {
    "@nestia/fetcher": ">= 1.5.3",
    "@nestjs/common": ">= 7.0.1",
    "@nestjs/core": ">= 7.0.1",
    "@nestjs/platform-express": ">= 7.0.1",
    "@nestjs/platform-fastify": ">= 7.0.1",
    "raw-body": ">= 2.0.0",
    "reflect-metadata": ">= 0.1.12",
    "rxjs": ">= 6.0.0",
    "typescript": ">= 4.8.0",
    "typia": ">= 4.1.14"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.17.15",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "commander": "^10.0.0",
    "comment-json": "^4.2.3",
    "eslint-plugin-deprecation": "^1.4.1",
    "git-last-commit": "^1.0.1",
    "inquirer": "^8.2.5",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.0",
    "tstl": "^2.5.13",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "lib",
    "src"
  ]
