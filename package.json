    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "nestia-helper": "^3.0.9",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "10.8.x",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^8.3.2"
  },
  "files": [
    "README.md",
    "LICENSE",
