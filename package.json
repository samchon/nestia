  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://github.com/samchon/nestia#readme",
  "dependencies": {
    "cli": "^1.0.1",
    "del": "^6.0.0",
    "glob": "^7.2.0",
    "nestia-fetcher": "^2.0.1",
    "path-to-regexp": "^6.2.1",
    "tgrid": "^0.8.7",
    "tsconfck": "^2.0.1",
    "tsconfig-paths": "^4.0.0",
    "tstl": "^2.5.12",
    "typescript-json": "^3.3.11"
